    <!--
        Added the entity WorkEstimateRateAnalysis.
    -->
    <changeSet id="20210623060754-1" author="jhipster">
        <createTable tableName="work_estimate_rate_analysis">
            <column name="id" type="bigint" autoIncrement="true">
