            </column>
            <column name="lift_charges" type="decimal(21,2)">
                <constraints nullable="true" />
            </column>
            <column name="locality_allowance" type="decimal(21,2)">
                <constraints nullable="true" />
            </column>
            <column name="employees_cost" type="decimal(21,2)">
                <constraints nullable="true" />
            </column>
            <column name="contingencies" type="decimal(21,2)">
                <constraints nullable="true" />
            </column>
            <column name="transportation_cost" type="decimal(21,2)">
                <constraints nullable="true" />
            </column>
            <column name="service_tax" type="decimal(21,2)">
                <constraints nullable="true" />
            </column>
            <column name="provident_fund_charges" type="decimal(21,2)">
