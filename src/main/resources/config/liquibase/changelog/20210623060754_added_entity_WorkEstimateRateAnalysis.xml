            </column>
            <column name="insurance_cost" type="decimal(21,2)">
                <constraints nullable="true" />
            </column>
            <column name="statutory_charges" type="decimal(21,2)">
                <constraints nullable="true" />
            </column>
            <column name="compensation_cost" type="decimal(21,2)">
                <constraints nullable="true" />
            </column>
            <column name="ra_completed_yn" type="boolean">
                <constraints nullable="false" />
            </column>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here -->
        </createTable>
    </changeSet>

    <!-- jhipster-needle-liquibase-add-changeset - JHipster will add changesets here -->

