            </column>
            <column name="sor_financial_year" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="basic_rate" type="decimal(21,2)">
                <constraints nullable="false" />
            </column>
            <column name="net_rate" type="decimal(21,2)">
                <constraints nullable="false" />
            </column>
            <column name="floor_no" type="bigint">
                <constraints nullable="true" />
